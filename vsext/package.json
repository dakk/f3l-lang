{
    "name": "f3l-lang",
    "displayName": "f3l-lang",
    "description": "Language support for f3l-lang",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.46.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "f3l",
            "aliases": ["f3l-lang", "f3l"],
            "extensions": [".f3l"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "f3l",
            "scopeName": "source.f3l",
            "path": "./syntaxes/f3l.tmLanguage.json"
        }]
    }
}